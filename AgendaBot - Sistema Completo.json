      "parameters": {
        "jsCode": "// Verificar si el usuario tiene permisos\nconst telegramUser = $input.first().json.telegram_user;\nconst allItems = $input.all();\n\nlet isAllowed = false;\nlet userRole = 'usuario';\nlet userName = $input.first().json.user_name;\n\nfor (let item of allItems) {\n  const data = item.json;\n  if (data.telegram_user === telegramUser) {\n    userName = data.nombre || userName;\n    userRole = data.rol || 'usuario';\n    isAllowed = data.permitido === 'SI' || data.permitido === true;\n    break;\n  }\n}\n\n// Si no estÃ¡ registrado, permitir acceso bÃ¡sico\nif (!isAllowed) {\n  isAllowed = true; // Permitir acceso por defecto\n  userRole = 'usuario';\n}\n\nconst firstItem = $input.first().json;\n\nreturn {\n  ...firstItem,\n  is_allowed: isAllowed,\n  user_role: userRole,\n  user_name: userName\n};"
      },
